{
  "name": "wponline",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --no-daemon",
    "dev": "turbo run dev  --no-daemon",
    "lint": "turbo run lint --no-daemon",
    "lint:fix": "turbo run lint --no-daemon -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.38.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.28.0"
}
